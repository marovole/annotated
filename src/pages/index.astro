---
import BaseLayout from '../layouts/BaseLayout.astro';
import PillarPageContent from '../components/PillarPageContent.astro';

const title = "Annotated Bibliography Example (MLA, APA 7 & Chicago) | annotated.help";
const description = "See annotated bibliography examples in MLA, APA 7, and Chicago styles. Copy and paste free templates for journals, books, and websites.";

// Canonical points to the pillar page to avoid duplicate content
const canonical = "https://annotated.help/annotated-bibliography-example/";
---

<BaseLayout title={title} description={description} canonical={canonical} pageType="pillar">
  <PillarPageContent />
</BaseLayout>
